<%- include('../partials/header') %>
<form class="edit-form" action="/skills/<%= skill.id %>?_method=PUT" method="POST" autocomplete="off">
  <label> Title
      <input type="text" name='skill' value="<%= skill.skill %>">
  </label>
  <label> Description
      <textarea name="description" cols="30" rows="10"><%= skill.description %></textarea>
  </label>
  <button class="update-btn" type="submit">Update Skill</button>
</form>
<%- include('../partials/footer') %>